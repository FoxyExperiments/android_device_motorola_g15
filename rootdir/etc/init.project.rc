import /vendor/etc/init/hw/init.mtkgki.rc

# MTK project .rc configure

on early-init
    exec_background u:r:vendor_modprobe:s0 -- /vendor/bin/modprobe -a -d /vendor/lib/modules trace_mmstat.ko

on init
    mkdir /mnt/media_rw/usbotg 0700 media_rw media_rw
    mkdir /storage/usbotg 0700 root root

on post-fs-data

#Camera
    chmod 0660 /dev/MAINAF
    chown system camera /dev/MAINAF

    chmod 0660 /dev/MAINAF2
    chown system camera /dev/MAINAF2

    chmod 0660 /dev/SUBAF
    chown system camera /dev/SUBAF

    chmod 0660 /dev/GAF001AF
    chown system camera /dev/GAF001AF

    chmod 0660 /dev/DW9714AF
    chown system camera /dev/DW9714AF

    chmod 0660 /dev/LC898212AF
    chown system camera /dev/LC898212AF

    chmod 0660 /dev/BU64745GWZAF
    chown system camera /dev/BU64745GWZAF

#SMB
    chown system system /proc/smb/ScreenComm
    chmod 0660 /proc/smb/ScreenComm

    chmod 0660 /dev/spm
    chown system system /dev/spm

    #yangyong add for NXP NFC begin
    #chmod 066 /dev/nxp-nci
    chmod 0777 /dev/nxp-nci
    chown nfc nfc /dev/nxp-nci
    #yangyong add for NXP NFC end

#feiyu.zhu modify for EKLAMU-1407
    chown system system /sys/devices/platform/1100a000.spi0/spi_master/spi0/spi0.0/irq
    chmod 0777 /sys/devices/platform/1100a000.spi0/spi_master/spi0/spi0.0/irq
    chown system system /sys/devices/platform/1100a000.spi0/spi_master/spi0/spi0.0/fpc_sys_ctl
    chmod 0777 /sys/devices/platform/1100a000.spi0/spi_master/spi0/spi0.0/fpc_sys_ctl
    chown system system /sys/devices/platform/1100a000.spi0/spi_master/spi0/spi0.0/hw_reset
    chmod 0777 /sys/devices/platform/1100a000.spi0/spi_master/spi0/spi0.0/hw_reset
    chown system system /sys/devices/platform/1100a000.spi0/spi_master/spi0/spi0.0/clk_enable
    chmod 0777 /sys/devices/platform/1100a000.spi0/spi_master/spi0/spi0.0/clk_enable
    chown system system /sys/devices/platform/1100a000.spi0/spi_master/spi0/spi0.0/compatible_all
    chmod 0777 /sys/devices/platform/1100a000.spi0/spi_master/spi0/spi0.0/compatible_all

    chmod 0777 /dev/goodix_fp
    chown system system /dev/goodix_fp

#Audio
    chown system system /sys/devices/platform/11004000.i2c/i2c-7/7-0058

#db modify for sar
    chown system root /sys/class/sar/calibrate

    #TN Start by jirui.li  20240831 EKLAMU-30
    chmod 0664 /sys/devices/platform/charger/turbo_power_mode
    chown system system /sys/devices/platform/charger/turbo_power_mode
    #TN End by jirui.li  20240831 EKLAMU-30

    #TN Start by jirui.li  20240831 EKLAMU-202 for demo mode
    chmod 0664 /sys/devices/platform/charger/demo_mode_limit
    chown system system /sys/devices/platform/charger/demo_mode_limit
    #TN End by jirui.li  20240831 EKLAMU-202 for demo mode

    #TN Start by jirui.li  20240831 EKLAMU-202
    chmod 0664 /sys/devices/platform/charger/enable_charger
    chown system system /sys/devices/platform/charger/enable_charger
    #TN End by jirui.li  20240831 EKLAMU-202

on init
    # Refer to http://source.android.com/devices/tech/storage/index.html
    # It said, "Starting in Android 4.4, multiple external storage devices are surfaced to developers through
    #           Context.getExternalFilesDirs(), Context.getExternalCacheDirs(), and Context.getObbDirs().
    #           External storage devices surfaced through these APIs must be a semi-permanent part of the device (such as an SD card slot in a battery compartment).
    #           Developers expect data stored in these locations to be available over long periods of time."
    # Therefore, if the target doesn't support sd hot-plugging (Ex: the SD card slot in a battery compartment), we need to export SECONDARY_STORAGE in 'boot' section
    #
    # export SECONDARY_STORAGE /storage/sdcard1

# TN modified by feiyu.zhu for EKALAMU
    chown system system /sys/devices/platform/regulator-vibrator/vibrator_duration

service fuse_usbotg /system/bin/sdcard -u 1023 -g 1023 -w 1023 -d /mnt/media_rw/usbotg /storage/usbotg
    class late_start
    disabled

# start mmstat
on property:sys.boot_completed=1
    mkdir /sys/kernel/tracing/instances/mmstat 711
    write /sys/kernel/tracing/instances/mmstat/tracing_on 0
    write /sys/kernel/tracing/instances/mmstat/buffer_size_kb 16
    write /sys/kernel/tracing/instances/mmstat/events/mmstat/enable 1
    write /sys/kernel/tracing/instances/mmstat/tracing_on 1

#demo mode
    #TN Start by jirui.li  20240831 EKLAMU-202 for demo mode
    on property:sys.retaildemo.enabled=1
        write  /sys/devices/platform/charger/demo_mode_limit  1
    on property:sys.retaildemo.enabled=0
        write  /sys/devices/platform/charger/demo_mode_limit  0
    #TN End by jirui.li  20240831 EKLAMU-202 for demo mode

    #TN Start by jirui.li  20240831 EKLAMU-202
    on property:sys.chargerstop=0
        write  /sys/devices/platform/charger/enable_charger  1
    on property:sys.chargerstop=1
        write  /sys/devices/platform/charger/enable_charger  0
    #TN End by jirui.li  20240831 EKLAMU-202

    #TN Start by jirui.li  20241022 EKLAMU-8909
    on property:sys.overchargestop.enabled=0
        write  /sys/devices/platform/charger/battery_protection_mode  0
    on property:sys.overchargestop.enabled=1
        write  /sys/devices/platform/charger/battery_protection_mode  1
    #TN End by jirui.li  20241022 EKLAMU-8909

